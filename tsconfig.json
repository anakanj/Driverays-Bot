{
  "exclude": [
    "Trash",
    "Compiled",
    "**/test.ts"
  ],
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "Node", 
    "resolveJsonModule": true, 
    "allowJs": true, 
    "outDir": "./Compiled", 
    "removeComments": true, 
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "strict": true, 
    "strictNullChecks": true, 
    "skipLibCheck": true, 
    "plugins": [
      {
        "name": "typescript-lit-html-plugin",
        "tags": [
          "html",
          "template"
        ]
      }
    ]
  }
}